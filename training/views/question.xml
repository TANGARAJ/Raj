<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>


		<record model="ir.ui.view" id="qns_form_id">
			<field name="name">Questions</field>
			<field name="model">process</field>
			<field name="arch" type="xml">
				<form string="Questions" version="7.0">
					<header>
						<button name="make_confirm" type="object" string="Confirm" />
						<button name="make_done" type="object" string="Done"
							states="confirm" />
						<button name="make_cancel" type="object" string="Cancel" />
						<field name="state" widget="statusbar" />
					</header>
					<sheet>
						<group col="4">
						
							<field name="abcd" />
							<field name="trainer"></field>
							<field name="qns1"></field>
							<field name="anss1234" placeholder="Type your batch here!"></field>
							<field name="process_ids"></field>
							<field name="current_time"></field>
							<field name="training_ids" />
							<field name="status"/>
							<button type="object" name="load_questions" string="Duration"></button>
							<button type="object" name="batch_time" string="Batch"></button>
							<button type="object" name="searching_checking" string="ChangeBatch"></button>
							<!-- <button type="object" name="onchange_next" string= "onnonn"/>
 -->
						</group>
					</sheet>
				</form>

			</field>
		</record>
			
		 <act_window
            id="action_client_realate"
            name="relate"
            res_model="training.details"
            src_model="random.process"/>
