<?xml version='1.0' encoding='UTF-8'?>
<openerp>
	<data>
		<record model="ir.ui.view" id="Training_form">
			<field name="name">Training</field>
			<field name="model">training.details</field>
			<field name="arch" type="xml">

				<form string="Training" version="7.0">
					<sheet>
						<group>
							<field name="name" style='width:40%%' />
							<field name="age" />
							<field name="course" style='width:20%%' />
							<field name='fee' />
							<field name="date" />
							<field name="gender" />
							<field name="status"/>
							<note>
							<group>
							<field name="gold"/>
							<field name="current_time"></field>
							<field name="description" style='width:20%%' />
							<button name="random" type="object" string="checking button"/>
							</group>
							</note>
							
							<field name="checking_id" widget="many2many_tags"
								placeholder="Tags..." />
							<button type="object" string="search" name="check_search"></button>
							<field name="process_ids">
							<tree>
								<field name="abcd" />
								<field name="trainer"></field>
								<field name="qns1"></field>
								<field name="anss1234"></field>
							</tree>
							</field>
						</group>

					</sheet>
				</form>
			</field>
		</record>

		<record id="ir_cron_scheduler" model="ir.cron">
			<field name="name">Test Scheduler</field>
			<field name="interval_number">1</field>
			<field name="interval_type">minutes</field>
			<field name="numbercall">-1</field>
			<field eval="False" name="doall" />
			<field eval="'training.details'" name="model" />
			<field eval="'test_scheduler'" name="function" />
			<field eval="'()'" name="args" />
		</record>	
	 
		
		
		<record id="action_server_demo" model="ir.actions.server">
            <field name="name">Demo Server Action</field>
            <field name="condition">True</field>
            <field name="type">ir.actions.server</field>
            <field name="model_id" ref="model_training_details" />
            <field name="state">code</field>
            <field name="code">self.example_method(cr, uid, context.get('active_ids', []), context=context)</field>
        </record>
		
		<record model="ir.values" id="action_server_example">
            <field name="name">action_server_example</field>
            <field name="action_id" ref="action_server_demo" />
            <field name="value" eval="'ir.actions.server,' + str(ref('action_server_demo'))" />
            <field name="key">action</field>
            <field name="model_id" ref="model_training_details" />
            <field name="model">training.details</field>
            <field name="key2">client_action_multi</field>
        </record>
        
		<record model="ir.actions.act_window" id="action_training">
			<field name="name">Training</field>
			<field name="res_model">training.details</field>
			<field name="view_mode">tree,form</field>
		</record>
		<menuitem name="TRAINING" id="menu_training" parent="menu_general"
			action="action_training" />
	</data>
</openerp>

	
        