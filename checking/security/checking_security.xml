<openerp>
	<data>
		<record model="ir.module.category" id="module_category_check">
			<field name="name">Thang Group Category</field>
			<field name="description">Helps you manage your shipping operations</field>
			<field name="sequence">5</field>
		</record>
		<record id="group_checking" model="res.groups">
			<field name="name">Checking Group</field>
			<field name="category_id" ref="module_category_check" />
		</record>
		<record id="group_checking_manager" model="res.groups">
			<field name="name">Manager</field>
			<field name="category_id" ref="module_category_check" />
			<field name="implied_ids" eval="[(4, ref('group_checking'))]" />
			<field name="users" eval="[(4, ref('base.user_root'))]" />
		</record>
	</data>
</openerp>