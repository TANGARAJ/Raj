<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>
		<record model="ir.module.category" id="module_category_school">
			<field name="name">school groups</field>
			<field name="description">Teachers access</field>
			<field name="sequence">5</field>
		</record>
		<record id="group_school_student" model="res.groups">
			<field name="name">student</field>
			<field name="category_id" ref="module_category_school" />
		</record>
 
		<record id="school_groups" model="res.groups">
			<field name="name">staff Group</field>
			<field name="category_id" ref="module_category_school" />
			
		</record>
			 <record id="group_school_management" model="res.groups">
			<field name="name">management</field>
			<field name="category_id" ref="module_category_school" />
			<field name="implied_ids" eval="[(4, ref('school_groups')),(4, ref('group_school_student'))]" />
		</record>

	</data>
</openerp> 