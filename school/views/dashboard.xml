<?xml version="1.0"?>
<openerp>
    <data>
        <record model="ir.actions.act_window" id="act_enrolment_graph">
            <field name="name">school</field>
            <field name="res_model">enrolment</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="school.enrol_id_4658"/>
        </record>
        <record model="ir.actions.act_window" id="act_enrolment_calendar">
            <field name="name">particulars</field>
            <field name="res_model">particulars.student</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="school.particulars_form_id"/>
        </record>
        <record model="ir.ui.view" id="board_enrolment_form">
            <field name="name">enrolment Dashboard Form</field>
            <field name="model">board.board</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="enrolment Dashboard">
                    <board style="2-1">
                        <column>
                            <action
                                string="School"
                                name="%(act_enrolment_graph)d"
                                height="150"
                                width="510"/>
                                </column>
                                <column>
                            <action
                                string="School"
                                name="%(act_enrolment_calendar)d"
                                height="150"
                                width="510"/>
                                </column>
                    </board>
                </form>
            </field>
        </record>
        <record model="ir.actions.act_window" id="open_board_enrolment">
          <field name="name">enrolmentDashboard</field>
          <field name="res_model">board.board</field>
          <field name="view_type">form</field>
          <field name="view_mode">form</field>
          <field name="usage">menu</field>
          <field name="view_id" ref="board_enrolment_form"/>
        </record>

        <menuitem   name="enrolment Dashboard" parent="base.menu_reporting_dashboard"
            action="open_board_enrolment"   id="menu_board_enrolment" icon="terp-graph"/>
    </data>
</openerp>

