<?xml version='1.0' encoding='UTF-8'?>
<openerp>
	<data>
		<record model="ir.ui.view" id="student_form_id">
			<field name="name">student details</field>
			<field name="model">student.regis</field>
			<field name="arch" type="xml">
				<form string="student dtails" version="7.0">
					<sheet>
						<group >
							<field  name='image142' widget="image" class="oe_left oe_avatar"/>
							<field name="serial_number"/>
							<field name="name" />
					<!-- 		<field name="surname" class="oe_subtotal_footer oe_right"/> -->
							<field name="standard" />
							<field name="age" />
							<field name="dob" />
							<field name="gender" />
							<field name="enrolment_number" />
							<field name="address" />
							<field name="state_id" />
							<field name="country_id" />
							<field name="adm_date" />
							<!-- <field name="nationality" />
							<field name="contact_number" /> -->
                             <button type="action" string="Enrolment form" name="%(school.enrolling_action_student_998)d" class="oe_highlight" />
						</group>

					</sheet>

				</form>
			</field>
			</record>
           <record model="ir.ui.view" id="student_tree_view">
                <field name="name">student details</field>
                <field name="model">student.regis</field>
                <field name="type">tree</field>
                <field name="arch" type="xml">
                    <tree string="tree_string">
                     <field name="serial_number"/>
							<field name="name" />
					<!-- 		<field name="surname" /> -->
							<field name="standard" />
							<field name="age" />
							<field name="dob" />
							<field name="gender" />
							<field name="enrolment_number" />
							<field name="address" />
							<field name="state_id" />
							<field name="country_id" />
							<field name="adm_date" />
                    </tree>
                </field>
            </record>
			<record model="ir.actions.act_window" id="student_action_id903">
				<field name="name">student details</field>
				<field name="res_model">student.regis</field>
				<field name="view_mode">tree,form</field>
			</record>

			<menuitem name="student" id="student_id55748" parent="student_details_id"
				action="student_action_id903" />
	</data>
</openerp>
        
         
         
         
         
         
         
         