<?xml version="1.0" encoding="UTF-8"?>

<openerp>
	<data>

		<record model="ir.ui.view" id="timetable_form_view">
			<field name="name">timetable.form</field>
			<field name="model">time.table</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="time_table">
					<sheet>



						<group col="4">
							<field name="timetable" />
							<field name="days" />
						
							<field name="hr1" />
			
							<field name="hr2" />
					
							<field name="hr3" />
						
							<field name="hr4" />
					
							<field name="hr5" />
                           	<field name="date" />
						</group>

					</sheet>
				</form>
			</field>
		</record>
		<record model="ir.ui.view" id="timetable_id_tree_view">
			<field name="name">timetable.form</field>
			<field name="model">time.table</field>
			<field name="type">tree</field>
			<field name="arch" type="xml">
				<tree string="timetable">
					<field name="timetable" />
					<field name="days" />
					<field name="date" />
					<field name="hr1" />
					<field name="hr2" />
					<field name="hr3" />
					<field name="hr4" />
					<field name="hr5" />
				</tree>
			</field>
		</record>



		<record model="ir.actions.act_window" id="action_time_table_id">
			<field name="name">timetable.form</field>
			<field name="res_model">time.table</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
		</record>
		<!-- <menuitem id="time_table_id" name="Time table" parent="student_details_id"
			action="action_time_table_id" /> -->

	</data>
</openerp>